void Usif() {
    ll T = 0;
    if (!(cin >> T)) return;   

    while (T--) {
        string s;
        cin >> s;
        if (s.length() >= 10)
            cout << "strong\n";
        else
            cout << "weak\n";
    }
}

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    Usif();
    return 0;
}
