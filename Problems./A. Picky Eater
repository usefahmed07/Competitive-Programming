void Usif() {
    int x , y; cin >> x >> y;
    if (x > y || x == y){
        cout <<"1"<<"\n";
    }
    else {
        cout <<"0"<<"\n";
    }
}
int main() {
    ios::sync_with_stdio(false); cin.tie(nullptr);
    Usif();
    return 0;
}

