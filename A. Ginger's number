#include <bits/stdc++.h>
using namespace std;
using ll = long long;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int T;
    cin >> T;

    while(T--) {
        ll x, y;
        cin >> x >> y;

        ll g = gcd(x, y);

        if(g == 1) {
            cout << 1 << "\n";
            continue;
        }

        ll ans = g;

        for(ll i = 2; i * i <= g; i++) {
            if(g % i == 0) {
                ans = i;
                break;
            }
        }

        cout << ans << "\n";
    }

    return 0;
}
